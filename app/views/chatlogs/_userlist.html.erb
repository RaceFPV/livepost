<!-- list all current users in this chat -->
<div class="col-sm-3" style="margin-top: 3%;padding-left:0px;">
  <div class="panel panel-default" style="padding-bottom: 5px;background-color: #111;border-color: #000;">
  <h3 align="center" style="color: #eaeaea;border-bottom: solid; border-color: #000;padding-bottom: 10px;"><b>Users here</b></h3>
    <div class="panel panel-body" style="background-color: #111;">
      <div style="height: 350px; overflow-y: auto;" align="center" id="userlist">
            
        <% @usershere.each do |x| %>
        <% @alreadylisted = false %>
        <% @chat.administrators.each do |z| %>
        <% if z == x.email and @alreadylisted == false and z != nil%>
        <p><%= link_to x.name, x %> <span class="label label-danger">Admin</span></p>
        <% @alreadylisted = true %>
        <% end %>
        <% if z != x.email and @alreadylisted == false and z != nil %>
        <p><%= link_to x.name, x %></p>
        <% @alreadylisted = true %>
        <% end %>
        <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>