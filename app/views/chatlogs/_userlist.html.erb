<!-- list all current users in this chat -->
<div class="col-sm-3" style="margin-top: 3%;padding-left:0px;">
  <div class="panel panel-default" style="padding-bottom: 4px;">
  <h3 align="center"><b>Users here</b></h3>
    <div class="panel panel-body" style="margin-left: 10px;margin-right:10px;">
      <div style="height: 362px; overflow-y: auto;" align="center" id="userlist">
        <% @usershere.each do |x| %>
          <% @alreadylisted = false %>
          <% if x.super_user == true %>
            <p><%= link_to x.name, x %> <span class="label label-danger">SuperAdmin</span></p>
          <% else %>
            <% @chat.administrators.each do |z| %>
              <% if z == x.id and @alreadylisted == false and z != nil%>
                <p><%= link_to x.name, x %> <span class="label label-danger">Admin</span></p>
                <% @alreadylisted = true %>
              <% end %>
              <% if z != x.email and @alreadylisted == false and z != nil %>
                <p><%= link_to x.name, x %></p>
                <% @alreadylisted = true %>
              <% end %>
            <% end %>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>